{
	"name": "g2p2p-playground",
	"version": "0.0.1",
	"scripts": {
		"start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"typecheck": "tsc --noEmit",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"format": "prettier --write \"{src,test,.}/*.{ts,js,json,mjs,yml,yaml,md}\"",
		"test": "jest --detectOpenHandles --config ./jest.json",
		"test:cov": "jest --detectOpenHandles --coverage --config ./jest.json",
		"clean": "rm -rf dist/",
		"build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
		"build:static": "webpack --config webpack.config.cjs",
		"start:prod": "node -r tsconfig-paths/register dist/index.js",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@types/config": "^3.3.5",
		"@types/cookie-parser": "^1.4.9",
		"@types/express": "^5.0.3",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24.5.2",
		"@types/validator": "^13.15.3",
		"cross-env": "^10.0.0",
		"css-loader": "^7.1.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^16.1.5",
		"mini-css-extract-plugin": "^2.9.4",
		"prettier": "^3.4.2",
		"sass": "^1.93.1",
		"sass-loader": "^16.0.5",
		"ts-jest": "^29.2.5",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"webpack": "^5.101.3",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"body-parser": "^2.2.0",
		"config": "^4.1.1",
		"cookie-parser": "^1.4.7",
		"dotenv": "^17.2.1",
		"effect": "^3.17.14",
		"express": "^5.1.0",
		"helmet": "^8.1.0",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"reflect-metadata": "^0.2.2",
		"sequelize": "^6.37.7",
		"sequelize-typescript": "^2.1.6",
		"sqlite3": "^5.1.7",
		"winston": "^3.17.0",
		"zod": "^3.23.8"
	}
}
